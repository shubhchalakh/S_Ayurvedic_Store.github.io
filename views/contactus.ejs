  <body class="d-flex h-100 text-center text-white bg-white">
      <div class="container d-flex w-100 h-100 p-3 mx-auto flex-column">
        <header class="mb-auto">
          <div>
            <h2 class="float-md-start mb-0 text-dark">S Ayurvedic Store</h2>
<p></p><nav class="nav nav-masthead justify-content-center float-md-center">
          <a class="nav-link fw-bold py-1 px-0 btn p=5 btn-dark text-white" href="/contactus" class="text-dark">Contact Us</a>
          </nav> 
            <nav class="nav nav-masthead justify-content-center float-md-end">
              
              <form action="/logout?_method=DELETE" method="POST">
                <button class="nav-link fw-bold py-1 px-0 btn p=3 btn-dark text-white" type="submit"  >Logout</button>

              </form>

              
              
            </nav>
          </div>
        </header>

        <main class="px-3">
            <form action="/contactus" method="POST">
               
                <div class="row">
                  
                  <% if (messages.error) { %>
                    <div class="alert alert-danger">
                      <strong><%= messages.error %> </strong>
                    </div>
                  <% } %> 
                  <h1 class="mb-3 h3">Contact Us</h1>
                  <div class="col-md-12 mb-4">
                    <div class="form-outline">
                      <input type="text" name="name" id="form3Example2" class="form-control" />
                      <label class="form-label" for="form3Example2">Full Name</label>
                    </div>
                  </div>
                </div>
  
                
                <div class="form-outline mb-4">
                  <input type="text" name="subject" id="form3Example3" class="form-control" />
                  <label class="form-label" for="form3Example3">Subject</label>
                </div>
  
                
                <div class="form-outline mb-4">
                  <input type="text" name="description" id="form3Example4" class="form-control" />
                  <label class="form-label" for="form3Example4">Description</label>
                </div>

                
                <button type="submit" class="btn btn-primary btn-block mb-4">
                  Submit
                </button>
  
                
                
              </form>
          
          
        </main>

        <footer class="mt-auto text-dark-50">
          <p class="text-dark">** <a href="" class="text-dark">Copyright</a>, by <a href="" class="text-dark">@shubhamchalakh</a>.</p>
